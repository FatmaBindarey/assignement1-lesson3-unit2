#include <stdio.h>
#include <stdlib.h>

int main(void)
{
	float z;
	float p;
printf( " enter two number :");
fflush(stdin); 
fflush(stdout);
scanf(" %f %f",&z,&p);
printf(" the product  is = %f ",z*p);
}
